# Application Settings
APP_NAME="Autonomite Agent Platform"
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-here
API_PREFIX=/api/v1

# Server Configuration
HOST=0.0.0.0
PORT=8000
WORKERS=4

# Database Configuration (Master Supabase)
SUPABASE_URL=https://your-master-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# LiveKit Configuration (Default)
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your-api-key
LIVEKIT_API_SECRET=your-api-secret

# Admin Configuration
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=changeme

# CORS Settings
CORS_ORIGINS=["http://localhost:3000", "https://yourdomain.com"]
CORS_ALLOW_CREDENTIALS=true

# JWT Settings
JWT_SECRET_KEY=your-jwt-secret-key
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=43200
WORDPRESS_BRIDGE_SECRET=
WORDPRESS_BRIDGE_MAX_SKEW=300

# API Keys (for default agents)
OPENAI_API_KEY=
GROQ_API_KEY=
DEEPINFRA_API_KEY=
REPLICATE_API_KEY=
DEEPGRAM_API_KEY=
ELEVENLABS_API_KEY=
CARTESIA_API_KEY=
SPEECHIFY_API_KEY=
NOVITA_API_KEY=
COHERE_API_KEY=
SILICONFLOW_API_KEY=
JINA_API_KEY=

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60

# Docker Settings
DOCKER_NETWORK=autonomite-network
AGENT_IMAGE_NAME=autonomite-agent:latest

# Monitoring
PROMETHEUS_ENABLED=false
PROMETHEUS_PORT=9090

# Knowledge Base Upload Limits
DOCUMENT_PROCESSOR_MAX_FILE_MB=100

# Backups
BACKUP_ENABLED=false
BACKUP_S3_BUCKET=
BACKUP_S3_ACCESS_KEY=
BACKUP_S3_SECRET_KEY=
BACKUP_RETENTION_DAYS=30

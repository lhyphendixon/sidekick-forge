# Master Supabase Configuration
# This is the master project that stores client configurations
MASTER_SUPABASE_URL=https://YOUR_MASTER_PROJECT.supabase.co
MASTER_SUPABASE_SERVICE_KEY=your-master-service-role-key

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Application Settings
APP_ENV=development
DEBUG=true

# API Settings
API_PREFIX=/api/v1
CORS_ORIGINS=["http://localhost:3000", "https://autonomite.net", "https://*.wordpress.com"]

# Security
SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090